#include <stdio.h>

//contando de 0 até o valor desejado pelo usuário.

int main()
{
    int contador, valor;
    
     contador = 0;
    
    printf ("Até quanto você deseja contar? ");
    scanf ("%d", &valor);
    
    while (contador <= valor) {
        printf ("%d\n", contador);
        contador = contador + 1;
        
    }

    return 0;
}
